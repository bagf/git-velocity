{
  "name": "git-velocity",
  "version": "1.0.0",
  "description": "CLI tool used to calculate git commit velocity",
  "main": "index.js",
  "repository": "https://github.com/ibarsi/git-velocity",
  "bin": {
    "git-velocity": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel main/ -s -D -d dist/",
    "debug": "npm run build && node --inspect=12345 --debug dist/index.js",
    "start": "npm run build && node dist/index.js"
  },
  "author": "ibarsi",
  "license": "ISC",
  "dependencies": {
    "async": "^2.1.4",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "figlet": "^1.2.0",
    "inquirer": "^3.0.1",
    "moment": "^2.17.1",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "nodemon": "^1.9.2"
  }
}
